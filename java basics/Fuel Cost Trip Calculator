 
import java.util.Scanner;
public class Basic{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int distance=sc.nextInt();
        int milege=sc.nextInt();
        int fuelprice=sc.nextInt();
        int tollcharge=sc.nextInt();
        int base=distance*milege;
        int bonus=base+fuelprice;
        int detuctcharge=bonus-tollcharge;
        System.out.println("Final Weekly Wage:"+detuctcharge);
        
        
    }
    
}
